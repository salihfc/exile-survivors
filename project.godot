[application]

config/name="Exile_Survivors"
run/main_scene="res://src/SceneManager.tscn"
config/icon="res://icon.png"

[autoload]

CONFIG="*res://src/singletons/Config.gd"
UTILS="*res://src/singletons/Utils.tscn"
LOG="*res://src/singletons/Log.gd"
AUDIO="*res://src/singletons/AudioManager.tscn"

[debug]

gdscript/warnings/treat_warnings_as_errors=true
gdscript/warnings/unused_class_variable=true
gdscript/warnings/unsafe_property_access=true
gdscript/warnings/unsafe_method_access=true
gdscript/warnings/unsafe_cast=true
gdscript/warnings/unsafe_call_argument=true

[display]

window/size/width=1920
window/size/height=1080
window/size/test_width=1728
window/size/test_height=972
window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor]

main_run_args="--verbose"

[editor_plugins]

enabled=PoolStringArray( "res://addons/light_snippet/plugin.cfg" )

